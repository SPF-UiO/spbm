{% extends "base.jinja" %}

{% block content %}
<table class="table table-striped">
<tr><th>Period</th><th>Amount</th><th>Paid?</th></tr>
{% for proc in invoices %}
<tr><td><a href="{{ proc.period }}.pdf">{{ proc.period }}</a></td><td>{{ proc.get_total_cost() }}</td><td>{{ proc.paid }}</td></tr>
{% endfor %}
</table>
{% endblock %}
